<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Admin_auth</servlet-name>
        <servlet-class>Admin_auth</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NewServlet</servlet-name>
        <servlet-class>com.example.NewServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>customer</servlet-name>
        <servlet-class>customer</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>jhvjh</servlet-name>
        <servlet-class>jhvjh</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>User_authentication</servlet-name>
        <servlet-class>User_authentication</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UserRegister</servlet-name>
        <servlet-class>UserRegister</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Admin_auth</servlet-name>
        <url-pattern>/Admin_auth</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NewServlet</servlet-name>
        <url-pattern>/NewServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>customer</servlet-name>
        <url-pattern>/customer</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>jhvjh</servlet-name>
        <url-pattern>/jhvjh</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Admin_authentication</servlet-name>
        <servlet-class>Admin_authentication</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Admin_authentication</servlet-name>
        <url-pattern>/Admin_authentication</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>User_authentication</servlet-name>
        <url-pattern>/User_authentication</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UserRegister</servlet-name>
        <url-pattern>/UserRegister</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
